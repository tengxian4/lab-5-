// this exercise is using selection control structure,ask the user to enter POSITIF value for x and z.

// 



#include "stdafx.h" 

# include <iostream>  

# include<cmath> 
#include <string>
using namespace std;



int main()

{
	int x, z;
	cout << "please enter positive number for x and z:";
	cin >> x >> z;
	if (x > 0 && z > 0)
		cout << "this is positive number\n ";
	else
		cout << " this is not positive number\n";


	string name;
	cout << "please enter a name :";
	cin >> name;
	getline(cin, name);

	return 0;

}
