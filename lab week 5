// this exercise is using selection control structure,ask the user to enter POSITIF value for x and z.

// 



#include "stdafx.h" 

# include <iostream>  

# include<cmath> 

using namespace std;



int main()

{
	int x, z;
	cout << "please enter positive number for x and z:";
	cin >> x >> z;
	if (x > 0 && z >0)
		cout << "this is positive number ";
	else
		cout << " this is not positive number\n";
	

	return 0;

}
